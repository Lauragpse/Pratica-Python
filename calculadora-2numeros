valor1 = int(input())
valor2 = int(input())
operadores = input()

if operadores == "+":
    print (valor1 + valor2)

elif operadores == "-":
    print (valor1 - valor2)


elif operadores == "*":
    print (valor1 * valor2)


elif operadores == "/":
    print (valor1 / valor2)


elif operadores == "//":
    print (valor1 // valor2)


elif operadores == "%":
    print (valor1 % valor2)


elif operadores == "**":
    print (valor1 ** valor2)
